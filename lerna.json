{
  "packages": [
    "components/*",
    "styles/*"
  ],
  "command": {
    "publish": {
      "allowBranch": "master",
      "message": "chore(release): publish %s"
    }
  },
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/*.test.js",
    "**/*.stories.jsx",
    "**/*.md"
  ],
  "version": "1.0.1"
}
