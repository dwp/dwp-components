{
  "packages": [
    "styles/*",
    "components/*"
  ],
  "command": {
    "publish": {
      "allowBranch": "master",
      "message": "chore(release): publish %s"
    }
  },
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/*.test.js",
    "**/*.stories.jsx",
    "**/*.md"
  ],
  "version": "0.0.0"
}
